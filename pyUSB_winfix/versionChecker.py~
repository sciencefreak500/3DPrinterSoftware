#!/usr/bin/env python
import sys


check = sys.version

if 'MSC' in check:
    if 'AMD' in check:
        if '32' in check:
            print("32 bit MSC AMD")
        elif '64' in check:
            print("64 bit MSC AMD")
    elif 'Intel' in check:
        if '32' in check:
            print("32 bit MSC Intel")
        elif '64' in check:
            print("64 bit MSC Intel")
